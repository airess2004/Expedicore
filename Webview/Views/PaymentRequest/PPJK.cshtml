    <!-- Dialog Agent (Agent, Shipper/Consignee) -->
    <div id="lookup_div_invoice_detail" class="easyui-dialog" title="Inc Agent" style="padding: 0px 5px; height: 470px; width: 720px;"
        data-options="iconCls:'icon-tip',modal:true">
        <div id="lookup_toolbar_invoice_detail" class="grid_toolbar">
            <a id="lookup_btn_add_invoice_detail" href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-save'">Save</a>
            <a id="lookup_btn_cancel_invoice_detail" href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-remove'">Close Window</a>
        </div>

        <table id="lookup_table_inc_agent" border="0" class="dummyClass ui-jqgrid-htable" cellpadding="0" cellspacing="0" width="100%">
            <tbody>
                <tr>
                    <td><span id="lblInvagentshipper">Contact</span></td>
                    <td colspan="4">
                        <input id="txtInvkdagent" type="text" size="5" maxlength="7" disabled="disabled" />
                        <input id="txtInvnmagent" type="text" size="56" maxlength="50" class="editable upper" disabled="disabled" />
                    </td>
                </tr>
                <tr>
                    <td>Account</td>
                    <td colspan="4">
                        <div style="float: left; width: 120px">
                            <input id="txtInvkdaccount" type="text" size="5" maxlength="7" disabled="disabled" />
                            <input id="btnInvaccount" name="buttonactivated" type="button" style="cursor: pointer;" class="ui-corner-all ui-state-default" value=".." />
                        </div>
                    </td>
                </tr>
                <tr>
                    <td valign="top">Account Name</td>
                    <td colspan="4">
                        <input id="txtInvnmaccount" type="text" size="80" maxlength="200" disabled="disabled" />
                    </td>
                </tr>
                <tr>
                    <td valign="top">Description</td>
                    <td colspan="4">
                        <textarea id="txtInvdesc" rows="5" cols="57"></textarea>
                    </td>
                </tr>
                <tr id="rowCurrencyType">
                    <td></td>
                    <td colspan="4">
                        <table style="border: none !important">
                            <tr style="border: none !important">
                                <td style="border: none !important">
                                    <label style="color: #ff6a00; font-weight: bold;">
                                        <input type="radio" name="optInvcurrencytype" id="optInvcurrencytypeUSD" checked="checked" />USD</label>
                                </td>
                                <td style="border: none !important">
                                    <label style="color: #0026ff; font-weight: bold;">
                                        <input type="radio" name="optInvcurrencytype" id="optInvcurrencytypeIDR" />IDR</label>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td nowrap style="white-space: nowrap">Quantity Unit</td>
                    <td colspan="4">
                        <input id="txtInvquantity" type="text" size="10" class="textright easyui-numberbox" data-options="precision:4,groupSeparator:','" value="0" />
                    </td>
                </tr>
                <tr>
                    <td nowrap style="white-space: nowrap">Per Unit Cost</td>
                    <td colspan="2">
                        <input id="txtInvperunitcost" type="text" size="10" class="textright easyui-numberbox" data-options="precision:2,groupSeparator:','" value="0" />
                    </td>
                    <td>Date Rate</td>
                    <td>
                        <input id="txtInvdaterate" type="text" size="10" disabled="disabled" />
                    </td>
                </tr>
                <tr>
                    <td>Amount</td>
                    <td width="30%">
                        <input id="txtInvamount" type="text" size="10" class="textright easyui-numberbox" data-options="precision:2,groupSeparator:','" value="0" />
                    </td>
                    <td>
                        <table style="border: none !important" width="100%" id="rowSign">
                            <tr style="border: none !important">
                                <td style="border: none !important">
                                    <label style="font-weight: bold;">
                                        <input type="radio" name="optInvsign" id="optInvsignplus" checked="checked" />+</label>
                                </td>
                                <td style="border: none !important">
                                    <label style="font-weight: bold;">
                                        <input type="radio" name="optInvsign" id="optInvsignminus" />-</label>
                                </td>
                            </tr>
                        </table>
                    </td>
                    <td>Exchange Rate</td>
                    <td>
                        <input id="txtInvexchangerate" type="text" size="10" disabled="disabled" />
                    </td>
                </tr>
                <tr id="rowVat" style="display: none;">
                    <td style="color: #ff6a00; font-weight: bold; font-size: 16px;">Vat</td>
                    <td width="30%" colspan="4">
                        <select id="ddlSEvat">
                            @if (Model.GetLookUp().Count == 1)
                            {
                                <option value="@Model.GetLookUp().FirstOrDefault().Id">
                                    @Model.GetLookUp().FirstOrDefault().Value
                                </option>
                            }
                            else
                            {
                                <option value="0"></option>
                                foreach (var c in Model.GetLookUp().ToList())
                                {
                                <option value="@c.Id">@c.Value</option>
                                }
                            }
                        </select>
                        &nbsp;%
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
    <!-- END Dialog Agent (Agent, Shipper/Consignee) -->

