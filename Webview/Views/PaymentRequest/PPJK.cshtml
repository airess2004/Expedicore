                                                <fieldset class="ui-widget-content ui-corner-all" style="width: 200px;">
                                                    <legend>Rate</legend>
                                                    <input id="txtSErate" class="textright easyui-numberbox" data-options="precision:2,groupSeparator:','" type="text" disabled="disabled" size="7" />
