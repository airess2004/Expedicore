                                             <td>Currency
                                            </td>
                                            <td>
                                                <select id="ddlcurrency">
                                                    <option value="USD">USD</option>
                                                    <option value="IDR">IDR</option>
                                                </select>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>Due Date</label></td>
                                            <td>
                                                <input id="txtSEduedate" type="text" disabled="disabled" size="10" maxlength="25"></td>
                                            <td rowspan="4" colspan="3">
                                                    <table >
                                                        <tr>
                                                            <td>
                                                                <label id="spaninvoiceto">Contact</label></td>
                                                            <td>
                                                                <input id="txtSEkdcustomer" class="textright" type="text" disabled="disabled" size="5" maxlength="25">
                                                                <input id="btnSEcustomer" name="buttonactivated" type="button" style="cursor: pointer;" class="ui-corner-all ui-state-default" value=".." />
                                                                <input id="txtSEnmcustomer" type="text" size="40" maxlength="50">
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td valign="top">Address</td>
                                                            <td>
                                                                <textarea id="txtSEcustomeraddress" rows="1" cols="45"></textarea>
                                                            </td>
                                                        </tr>
                                                    </table>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>ETD / ETA</label></td>
                                            <td>
                                                <input id="txtSEetd" type="text" disabled="disabled" size="10" maxlength="25"></td>
                                        </tr>
                                        @*<tr>
                                            <td valign="top">
                                                <label>Invoice Header</label></td>
                                            <td valign="top">
                                                <select id="ddlSEinvoiceheader">
                                                    @if (Model.JobOwnerList.ToList().Count == 1)
                                                    {
                                                        <option value="@Model.JobOwnerList[0].Id">
                                                            @Model.JobOwnerList[0].JobOwnerCode
                                                        </option>
                                                    }
                                                    else
                                                    {
                                                        <option value="0"></option>
                                                        foreach (var c in Model.JobOwnerList.ToList())
                                                        {
                                                        <option value="@c.Id">@c.JobOwnerCode</option>
                                                        }
                                                    }
                                                </select>
                                            </td>
                                        </tr>*@
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <fieldset class="ui-widget-content ui-corner-all">
                                        <legend>Being For Payment</legend>
                                        <div style="margin: 7px 0px !important;">
                                            <table id="table_invoice_detail" class="scroll" cellpadding="0" cellspacing="0"></table>
                                        </div>
                                        <input type="button" id="btn_add_invoice_detail" value="Add" class="btnSubmit" />
                                        <input type="button" id="btn_edit_invoice_detail" value="Edit" class="btnSubmit" />
                                        <input type="button" id="btn_delete_invoice_detail" value="Delete" class="btnSubmit" />
                                    </fieldset>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <table width="100%">
                                        <tr>
                                            <td width="80px">
                                                <fieldset class="ui-widget-content ui-corner-all" style="width: 80px;">
                                                    <table width="100%">
                                                        <tr>
                                                            <td>
                                                                <label>
                                                                    <input type="radio" name="rbTotalIDR" id="rbTotalIDRPaid" disabled="disabled" />Paid</label>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>
                                                                    <input type="radio" name="rbTotalIDR" id="rbTotalIDRNotPaid" disabled="disabled" checked="checked" />Not Paid</label>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </fieldset>
                                            </td>
                                            <td>
                                                <fieldset class="ui-widget-content ui-corner-all" style="width: 100px; float: left; margin-right: 5px;">
                                                    <legend>Payment USD</legend>
                                                    <input id="txtSEpaymentusd" class="textright easyui-numberbox" size="15" data-options="precision:2,groupSeparator:','" type="text" disabled="disabled" />
                                                </fieldset>
                                                <fieldset class="ui-widget-content ui-corner-all" style="width: 100px; float: left; margin-right: 5px;">
                                                    <legend>Payment IDR</legend>
                                                    <input id="txtSEpaymentidr" class="textright easyui-numberbox" size="15" data-options="precision:2,groupSeparator:','" type="text" disabled="disabled" />
                                                </fieldset>
                                            </td>
