        data-options="iconCls:'icon-tip',modal:true">

        <div id="lookup_toolbar_agent" class="grid_toolbar">
            <a id="lookup_btn_add_agent" href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-add'">Select Data</a>
            <a id="lookup_btn_cancel_agent" href="javascript:void(0)" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-remove'">Close Window</a>
        </div>

        <table id="lookup_table_agent" class="scroll" cellpadding="0" cellspacing="0"></table>
